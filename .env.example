# Varity App Store Environment Variables

# thirdweb Client ID (required for contract interactions)
# Get your Client ID from: https://thirdweb.com/dashboard
NEXT_PUBLIC_THIRDWEB_CLIENT_ID=

# Varity App Registry Contract Address
# Set after deploying the VarityAppRegistry contract
NEXT_PUBLIC_VARITY_REGISTRY_ADDRESS=

# Varity L3 Network Configuration (optional, defaults are provided)
NEXT_PUBLIC_VARITY_L3_RPC=https://rpc-varity-testnet-rroe52pwjp.t.conduit.xyz
NEXT_PUBLIC_VARITY_L3_CHAIN_ID=33529

# Varity Payments Contract Address (Arbitrum One)
# Set after deploying the VarityPayments contract to Arb One
NEXT_PUBLIC_VARITY_PAYMENTS_ADDRESS=
